				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_BACTERIA ) {
					if ( cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "bacteria" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_NOT_BACTERIA ) {
					if ( cardn@tcg(TCG_CARDN_REF_RACE, cnt) == "bacteria" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_PROJET ) {
					if ( cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "snail" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "wasp" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "beetle" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "spider" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "worm" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_BALZAK ) {
