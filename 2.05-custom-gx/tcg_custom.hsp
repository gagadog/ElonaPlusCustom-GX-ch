				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_DOG_OR_CAT ) {
					if ( cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "dog" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "cat" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "doggod" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "catgod" & card@tcg(TCG_CARD_REF_DBID, cnt) != CREATURE_ID_OLDER_DOG_SISTER & card@tcg(TCG_CARD_REF_DBID, cnt) != CREATURE_ID_YOUNGER_CAT_SISTER & card@tcg(TCG_CARD_REF_DBID, cnt) != CREATURE_ID_CAT_ZOMBIE & card@tcg(TCG_CARD_REF_DBID, cnt) != CREATURE_ID_BLACK_CAT ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_DRAGONESQUE ) {
					if ( cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "lizardman" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "dragon" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "drake" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "dinosaur" ) { continue }
