				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_RESURRECTABLE ) {
					if (effref@tcg(TCG_SKILL_EFF, card@tcg(TCG_CARD_EFFECT, cnt)) != TCG_SKILL_EFF_RESURRECT ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_BATTLECRY ) {
					if ( effref@tcg(TCG_SKILL_TYPE, card@tcg(TCG_CARD_EFFECT, cnt)) != TCG_SKILL_TYPE_BATTLECRY ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_CITIZEN ) {
					if ( cdbit(TCG_BIT_CITIZEN, cnt) != TRUE ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_HOUND ) {
					if ( card@tcg(TCG_CARD_REF_DBID, cnt) > CREATURE_ID_CHAOS_HOUND | card@tcg(TCG_CARD_REF_DBID, cnt) < CREATURE_ID_HOUND ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_HOUND_EFFECT ) {
					if ( card@tcg(TCG_CARD_REF_DBID, cnt) >= CREATURE_ID_CHAOS_HOUND | card@tcg(TCG_CARD_REF_DBID, cnt) <= CREATURE_ID_HOUND ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_ZOMBIE_GREATER ) {
					if ( (cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "zombie" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "skeleton" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "armor" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "lich") | card@tcg(TCG_CARD_REF_DBID, cnt) == CREATURE_ID_ANCIENT_COFFIN | card@tcg(TCG_CARD_REF_DBID, cnt) == CREATURE_ID_CURSED_COFFIN | card@tcg(TCG_CARD_REF_COST, cnt) <= 2 | card@tcg(TCG_CARD_REF_COST, cnt) > 5 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_ZOMBIE_LESSER ) {
					if ( (cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "zombie" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "skeleton" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "armor" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "lich") |  card@tcg(TCG_CARD_REF_DBID, cnt) == CREATURE_ID_ANCIENT_COFFIN | card@tcg(TCG_CARD_REF_DBID, cnt) == CREATURE_ID_CURSED_COFFIN | card@tcg(TCG_CARD_REF_COST, cnt) > 2 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_UNDEAD ) {
					if ( (cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "zombie" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "skeleton" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "armor" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "lich") ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_PIECE ) {
					if ( cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "piece" & card@tcg(TCG_CARD_REF_DBID, cnt) != CREATURE_ID_WYNAN_THE_LORD_OF_THE_ANCIENT_CASTLE ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_RAPHAEL ) {
					if ( cardn@tcg(TCG_CARDN_REF_SEX, cnt) != "female" & cardn@tcg(TCG_CARDN_REF_RACE, cnt) != "rat" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_NEUTRAL ) {
					if ( card@tcg(TCG_CARD_REF_DOMAIN, cnt) != 4 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_LEGENDARY ) {
					if ( card@tcg(TCG_CARD_REF_DOMAIN, cnt) != 5 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_NON_LEGENDARY ) {
					if ( card@tcg(TCG_CARD_REF_DOMAIN, cnt) == 5 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_BLUE ) {
					if ( card@tcg(TCG_CARD_REF_DOMAIN, cnt) != 0 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_GREEN ) {
					if ( card@tcg(TCG_CARD_REF_DOMAIN, cnt) != 1 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_WHITE ) {
					if ( card@tcg(TCG_CARD_REF_DOMAIN, cnt) != 2 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_BLACK ) {
					if ( card@tcg(TCG_CARD_REF_DOMAIN, cnt) != 3 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_GRAY ) {
					if ( card@tcg(TCG_CARD_REF_DOMAIN, cnt) != 6 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_RED ) {
					if ( card@tcg(TCG_CARD_REF_DOMAIN, cnt) != 7 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_WEREWOLF ) {
					if ( card@tcg(TCG_CARD_COST, cnt) != card@tcg(TCG_CARD_COST, effac@tcg) | card@tcg(TCG_CARD_REF_DOMAIN, cnt) != 4 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_SAMECOST ) {
					if ( card@tcg(TCG_CARD_COST, cnt) != card@tcg(TCG_CARD_COST, effac@tcg) ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) >= TCG_SKILL_FILTER_COST & effref@tcg(TCG_SKILL_FILTER, eff@tcg) < TCG_SKILL_FILTER_COST + 20 ) {
					if ( card@tcg(TCG_CARD_COST, cnt) != effref@tcg(TCG_SKILL_FILTER, eff@tcg) - TCG_SKILL_FILTER_COST ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) >= TCG_SKILL_FILTER_HP & effref@tcg(TCG_SKILL_FILTER, eff@tcg) < TCG_SKILL_FILTER_HP + 20 ) {
					if ( card@tcg(TCG_CARD_HP, cnt) != effref@tcg(TCG_SKILL_FILTER, eff@tcg) - TCG_SKILL_FILTER_HP ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) >= TCG_SKILL_FILTER_ATK & effref@tcg(TCG_SKILL_FILTER, eff@tcg) < TCG_SKILL_FILTER_ATK + 20 ) {
					if ( card@tcg(TCG_CARD_ATTACK, cnt) != effref@tcg(TCG_SKILL_FILTER, eff@tcg) - TCG_SKILL_FILTER_ATK ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) >= TCG_SKILL_FILTER_ATKGREATEROREQUAL & effref@tcg(TCG_SKILL_FILTER, eff@tcg) < TCG_SKILL_FILTER_ATKGREATEROREQUAL + 20 ) {
					if ( card@tcg(TCG_CARD_ATTACK, cnt) < effref@tcg(TCG_SKILL_FILTER, eff@tcg) - TCG_SKILL_FILTER_ATKGREATEROREQUAL ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) >= TCG_SKILL_FILTER_BIT & effref@tcg(TCG_SKILL_FILTER, eff@tcg) < TCG_SKILL_FILTER_BIT + 200 ) {
					if ( cdbit(effref@tcg(TCG_SKILL_FILTER, eff@tcg) - TCG_SKILL_FILTER_BIT, cnt) != 1 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) >= TCG_SKILL_FILTER_DBID & effref@tcg(TCG_SKILL_FILTER, eff@tcg) < TCG_SKILL_FILTER_DBID + MAX_CARD ) {
					if ( card@tcg(TCG_CARD_REF_DBID, cnt) != effref@tcg(TCG_SKILL_FILTER, eff@tcg) - TCG_SKILL_FILTER_DBID ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) >= TCG_SKILL_FILTER_DBID_NOTSELF & effref@tcg(TCG_SKILL_FILTER, eff@tcg) < TCG_SKILL_FILTER_DBID_NOTSELF + MAX_CARD ) {
					if ( card@tcg(TCG_CARD_REF_DBID, cnt) != effref@tcg(TCG_SKILL_FILTER, eff@tcg) - TCG_SKILL_FILTER_DBID_NOTSELF | cnt == effac@tcg ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_ELEAREFUGEEDIFFID ) {
					if ( cdbit(TCG_BIT_ELEAREFUGEE, cnt) != TRUE | card@tcg(TCG_CARD_COST, cnt) != card@tcg(TCG_CARD_COST, effac@tcg) | card@tcg(TCG_CARD_REF_DBID, cnt) == card@tcg(TCG_CARD_REF_DBID, effac@tcg) ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_ELEAREFUGEELOWCOST ) {
					if ( cdbit(TCG_BIT_ELEAREFUGEE, cnt) != TRUE | card@tcg(TCG_CARD_COST, cnt) >= 5 ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_WARRIOR ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "warrior" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_THIEF ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "thief" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_WIZARD ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "wizard" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_CLAYMORE ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "claymore" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_PRIEST ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "priest" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_ARCHER ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "archer" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_WARMAGE ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "warmage" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_GUNNER ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "gunner" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_PREDATOR ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "predator" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_FARMER ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "farmer" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_PIANIST ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "pianist" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_TOURIST ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "tourist" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_CLASSLESS ) {
					if ( cardn@tcg(TCG_CARDN_REF_CLASS, cnt) != "" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_MALE ) {
					if ( cardn@tcg(TCG_CARDN_REF_SEX, cnt) != "male" ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_FEMALE ) {
					if ( cardn@tcg(TCG_CARDN_REF_SEX, cnt) != "female" ) { continue }
				}
				if ( targettype@tcg \ 10 != 0 & cdbit(TCG_BIT_EVASION, cnt) & card@tcg(TCG_CARD_CONTROLLER, effac@tcg) != card@tcg(TCG_CARD_CONTROLLER, cnt) ) {
					has_evasion_target@tcg = 1
					continue
				}
				if ( targettype@tcg \ 10 == 0 | targettype@tcg \ 10 == 1 ) {
					target@tcg(target_num@tcg) = cnt
					target_num@tcg++
					continue
				}
				if ( targettype@tcg \ 10 >= 2 ) { // Reservoir sampling because I am both dumb and lazy
					target_samp@tcg++
					if ( target_num@tcg < targettype@tcg \ 10 - 1 ) {
						target@tcg(target_num@tcg) = cnt
						target_num@tcg++
					} else {
						p@tcg = rnd(target_samp@tcg)
						if ( p@tcg < targettype@tcg \ 10 - 1 ) {
							target@tcg(p@tcg) = cnt
						}
					}
				}
			loop
		}

		if ( target_num@tcg == 0 ) { continue }
		if ( dbg_tcg ) {
			poptext@tcg = "tcg effchoosetarget: targetnum:" + target_num@tcg + " targets:"
			repeat target_num@tcg
				poptext@tcg += "" + target@tcg(cnt) + "/" 
			loop
			if ( target@tcg(0) != (-1) ) { 
				proctcg poptext@tcg 
			}
		}

		// If there's only one target, might as well as choose automatically.
		if ( target_num@tcg == 1 & targettype@tcg >= 100 & targetloc@tcg >= 1 & targettype@tcg \ 10 == 1 ) { targettype@tcg += 1 }

		// Player choose
		if ( targettype@tcg >= 100 & targetloc@tcg >= 1 & targettype@tcg \ 10 == 1 ) {
			displayinfo@tcg = 1
			if ( (targetplayer@tcg != 1 & (targetloc@tcg == 2 | targetloc@tcg == 5)) | targetloc@tcg == 3 | targetloc@tcg == 4 | targetloc@tcg >= 6 | effref@tcg(TCG_SKILL_FILTER, eff@tcg) != TCG_SKILL_FILTER_NONE | has_evasion_target@tcg ) {
				// Cannot select from current view, use *select_deck to display card list,
				deckmode@tcg = targetloc@tcg
				deckmodecp@tcg = targetplayer@tcg - 1
				gosub *select_deck
				// Reset all card display status
				repeat maxcard@tcg
					if ( card@tcg(TCG_CARD_LOCATION, cnt) == TCG_LOCATION_HAND & card@tcg(TCG_CARD_CONTROLLER, cnt) != player@tcg ) {
						cdbitmod TCG_BIT_FACE_UP, cnt, 0
					}
					if ( card@tcg(TCG_CARD_LOCATION, cnt) == TCG_LOCATION_DECK | card@tcg(TCG_CARD_LOCATION, cnt) == TCG_LOCATION_GRAVEYARD | card@tcg(TCG_CARD_LOCATION, cnt) == TCG_LOCATION_EXILE ) {
						cdbitmod TCG_BIT_FACE_UP, cnt, 0
					}
				loop
				cardpos 0, -1
				cardpos 1, -1
				cardposhand 0, -1
				cardposhand 1, -1
				cardposgrave -1
				deckmode@tcg = 0
				deckmodecp@tcg = 0
			} else {
				// Setup which lines are selectable.
				cslinebk@tcg = csline@tcg
				selectmodebk@tcg = selectmode@tcg
				targetline@tcg = csline@tcg
				targetlinenum@tcg = 1
				if ( targetloc@tcg == 1 ) { 
					targetline@tcg = 2 - ((card@tcg(TCG_CARD_CONTROLLER, effac@tcg) == 0) ^ (targetplayer@tcg == 2)), 1 + ((card@tcg(TCG_CARD_CONTROLLER, effac@tcg) == 0) ^ (targetplayer@tcg == 2))
				} 
				if ( targetloc@tcg == 2 ) { 
					targetline@tcg = 3 - 3 * ((card@tcg(TCG_CARD_CONTROLLER, effac@tcg) == 0) ^ (targetplayer@tcg == 2)), 0 + 3 * ((card@tcg(TCG_CARD_CONTROLLER, effac@tcg) == 0) ^ (targetplayer@tcg == 2))
				} 
				if ( targetloc@tcg == 5 ) { 
					targetline@tcg = 3 - 3 * ((card@tcg(TCG_CARD_CONTROLLER, effac@tcg) == 0) ^ (targetplayer@tcg == 2)), 2 - ((card@tcg(TCG_CARD_CONTROLLER, effac@tcg) == 0) ^ (targetplayer@tcg == 2)), 1 + ((card@tcg(TCG_CARD_CONTROLLER, effac@tcg) == 0) ^ (targetplayer@tcg == 2)), 0 + 3 * ((card@tcg(TCG_CARD_CONTROLLER, effac@tcg) == 0) ^ (targetplayer@tcg == 2))
					targetlinenum@tcg = 2
				}
				if ( targetplayer@tcg == 3 ) {
					targetlinenum@tcg *= 2
				}
				csline@tcg = targetline@tcg(0)
				targetcs@tcg = 0
				selectmode@tcg = 2
				cs@tcg = 0
				gosub *card_player
				selectmode@tcg = selectmodebk@tcg
				csline@tcg = cslinebk@tcg
			}
			target_num@tcg = 1
			if ( rtval@tcg >= 0 & rtval@tcg < MAX_CARD) {
				target@tcg = rtval@tcg
			} else {
				target@tcg = target@tcg(rnd(target_num@tcg))
			}
		}

		// Adding selected targets to effect list.
		repeat target_num@tcg
			if ( targettype@tcg == TCG_SKILL_TARGET_NONE ) { target@tcg(cnt) = card@tcg(TCG_CARD_CONTROLLER, effac@tcg) }
			if ( targettype@tcg == TCG_SKILL_TARGET_PLAYER ) { target@tcg(cnt) = card@tcg(TCG_CARD_CONTROLLER, effac@tcg) }
			if ( targettype@tcg == TCG_SKILL_TARGET_ENEMY ) { target@tcg(cnt) = 1 - card@tcg(TCG_CARD_CONTROLLER, effac@tcg) }
			if ( targettype@tcg == TCG_SKILL_TARGET_SELF ) { target@tcg(cnt) = effac@tcg }
			if ( targettype@tcg == TCG_SKILL_TARGET_COMBAT_OPPONENT & attackingcard@tcg == effac@tcg ) { target@tcg(cnt) = attacktarget@tcg }
			if ( targettype@tcg == TCG_SKILL_TARGET_COMBAT_OPPONENT & attacktarget@tcg == effac@tcg ) { target@tcg(cnt) = attackingcard@tcg }
			if ( targettype@tcg == TCG_SKILL_TARGET_SELF_AND_COMBAT_OPPONENT & cnt == 1 & attackingcard@tcg == effac@tcg ) { target@tcg(cnt) = attacktarget@tcg }
			if ( targettype@tcg == TCG_SKILL_TARGET_SELF_AND_COMBAT_OPPONENT & cnt == 1 & attacktarget@tcg == effac@tcg ) { target@tcg(cnt) = attackingcard@tcg }
			if ( target@tcg(cnt) == (-1) ) { continue }

			cardstack@tcg(TCG_STACK, stack@tcg) = effac@tcg, effref@tcg(TCG_SKILL_EFF, eff@tcg), card@tcg(TCG_CARD_CONTROLLER, effac@tcg), eff@tcg, target@tcg(cnt), effref@tcg(TCG_SKILL_POWER, eff@tcg), cnt
			stack@tcg++
			efffired@tcg = 1
		loop
	loop
	if ( efffired@tcg == 0 ) {
		return
	}
	actionproc
	makecardlist
	return

