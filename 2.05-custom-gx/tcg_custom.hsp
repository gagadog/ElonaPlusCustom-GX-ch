*card_resolve_ongoing
	if ( chainmode@tcg != 0 | selectmode@tcg == 2 ) { return }
	repeat maxcard@tcg
		card@tcg(TCG_CARD_COST, cnt) = limit(card@tcg(TCG_CARD_REF_COST, cnt), 0, TCG_STAT_MAX)
		card@tcg(TCG_CARD_REF_ATTACK, cnt) = limit(card@tcg(TCG_CARD_REF_ATTACK, cnt), 0, TCG_STAT_MAX)
		card@tcg(TCG_CARD_ATTACK, cnt) = limit(card@tcg(TCG_CARD_REF_ATTACK, cnt), 0, TCG_STAT_MAX)
		card@tcg(TCG_CARD_REF_HP, cnt) = limit(card@tcg(TCG_CARD_REF_HP, cnt), 0, TCG_STAT_MAX)
		if ( card@tcg(TCG_CARD_HP, cnt) > card@tcg(TCG_CARD_REF_HP, cnt) ) {
			card@tcg(TCG_CARD_HP, cnt) = card@tcg(TCG_CARD_REF_HP, cnt)
		}
	loop
	repeat maxcard@tcg
		if ( card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_FIELD ) {
			continue
		}
		if ( cdbit(TCG_BIT_RIDER, cnt) ) {
			card@tcg(TCG_CARD_ATTACK, cnt)++
		}
		effac@tcg = cnt
		eff@tcg = card@tcg(TCG_CARD_EFFECT, cnt)
		if ( effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_ONGOING | effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_ONGOINGHANDFIELD ) {
			gosub *card_action_choose_target
		}
	loop
	repeat maxcard@tcg
		if ( card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_HAND ) {
			continue
		}
		effac@tcg = cnt
		eff@tcg = card@tcg(TCG_CARD_EFFECT, cnt)
		if ( effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_ONGOINGHAND | effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_ONGOINGHANDFIELD ) {
			cdbitmod TCG_BIT_FACE_UP, effac@tcg, 1
			gosub *card_action_choose_target
		}
	loop
	return

*card_action_choose_target
	proc "tcg efffire: " + " eff:" + eff@tcg + " effac:" + effac@tcg
	if ( cdbit(TCG_BIT_SILENCED, effac@tcg) ) {
		return
	}
	if ( cdbit(TCG_BIT_CONFUSED, effac@tcg) ) {
		return
	}
	if ( cdbit(TCG_BIT_FOSSIL, effac@tcg) ) {
		return
	}
	if ( (effref@tcg(TCG_SKILL_TYPE, eff@tcg) >= TCG_SKILL_TYPE_BATTLECRY & effref@tcg(TCG_SKILL_TYPE, eff@tcg) <= TCG_SKILL_TYPE_INHAND) | effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_AFTERCOMBAT ) {
		efllistaddcard 3, effac@tcg
	}
	if ( specialrulenoonedies@tcg & effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_DEATHRATTLE ) {
		return
	}
	if ( cdbit(TCG_BIT_DEVIL, effac@tcg) ) {
		dmgplayer card@tcg(TCG_CARD_CONTROLLER, effac@tcg), 2
	}
	if ( effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_BATTLECRY & effref@tcg(TCG_SKILL_EFF, eff@tcg) != TCG_SKILL_EFF_ANDROID ) {
		specialrulelastbattlecry@tcg = eff@tcg
	}
	makecardlist
	targettype@tcg = effref@tcg(TCG_SKILL_TARGET, eff@tcg)
	targetloc@tcg = (targettype@tcg / 10) \ 10
	targetplayer@tcg = targettype@tcg / 100

	// If opponent choose, he choose randomly.
	if ( (specialruleaitakeover@tcg > 0 | card@tcg(TCG_CARD_CONTROLLER, effac@tcg) != player@tcg) & targettype@tcg >= 100 & targetloc@tcg >= 1 & targettype@tcg \ 10 == 1 ) { targettype@tcg += 1 }

	efffirerepeat@tcg = limit(effref@tcg(TCG_SKILL_REPEAT, eff@tcg), 1, 10)
	if ( effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_BATTLECRY & specialrulebattlecrytwice@tcg > 0 ) {
		specialrulebattlecrytwice@tcg-- 
		efffirerepeat@tcg *= 2
	}
	
	efffired@tcg = 0
	repeat efffirerepeat@tcg
		makecardlist
		target@tcg = (-1)
		target_num@tcg = 1
		has_evasion_target@tcg = 0
		
		if ( targettype@tcg == TCG_SKILL_TARGET_PLAYER_AND_ENEMY ) { 
			target@tcg(0) = 0, 1
			target_num@tcg = 2
		}
		if ( targettype@tcg == TCG_SKILL_TARGET_SELF_AND_COMBAT_OPPONENT ) { 
			target@tcg(0) = effac@tcg, -1
			target_num@tcg = 2
		}

		// Extra effect for rachels books
		if ( eff@tcg >= TCG_EFF_RACHELSBOOK1 & eff@tcg <= TCG_EFF_RACHELSBOOK4 ) {
			cardstack@tcg(TCG_STACK, stack@tcg) = effac@tcg, TCG_SKILL_EFF_RACHAEL_BOOKS_HANDLE, card@tcg(TCG_CARD_CONTROLLER, effac@tcg), TCG_EFF_RACHELSBOOKHANDLE, card@tcg(TCG_CARD_CONTROLLER, effac@tcg), eff@tcg - TCG_EFF_RACHELSBOOK1 + 1, cnt
			stack@tcg++
			efffired@tcg = 1
		}

		// Enumerate through all cards to find target.
		if ( targettype@tcg >= 100 & targetloc@tcg >= 1 ) {
			target_num@tcg = 0
			target_samp@tcg = 0
			repeat maxcard@tcg
				if ( targetplayer@tcg == 1 & card@tcg(TCG_CARD_CONTROLLER, cnt) != card@tcg(TCG_CARD_CONTROLLER, effac@tcg) ) { continue } // Player's
				if ( targetplayer@tcg == 2 & card@tcg(TCG_CARD_CONTROLLER, cnt) != 1 - card@tcg(TCG_CARD_CONTROLLER, effac@tcg) ) { continue } // Opponent's
				if ( targetloc@tcg == 1 & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_FIELD ) { continue } // on Field
				if ( targetloc@tcg == 2 & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_HAND ) { continue } // in Hand
				if ( targetloc@tcg == 3 & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_GRAVEYARD ) { continue } // in Grave
				if ( targetloc@tcg == 4 & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_DECK ) { continue } // in Deck
				if ( targetloc@tcg == 5 & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_FIELD & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_HAND ) { continue } // in Hand / on Field
				if ( targetloc@tcg == 6 & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_DECK & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_HAND ) { continue } // in Hand / Deck
				if ( targetloc@tcg == 7 & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_DECK & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_HAND & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_FIELD  & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_GRAVEYARD ) { continue } // Everywhere
				if ( targetloc@tcg == 9 & card@tcg(TCG_CARD_LOCATION, cnt) != TCG_LOCATION_EXILE ) { continue } // in Deck
				
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_NOTYOURS ) {
					if ( targetplayer@tcg == 1 & cnt < MAX_CARD_DECK_TOTAL ) { continue }
					if ( targetplayer@tcg == 2 & cnt >= MAX_CARD_DECK_TOTAL ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_GENERATEDCARDS ) {
					if ( (cnt\MAX_CARD_DECK_TOTAL) < MAX_CARD_DECK ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_NOTSELF ) {
					if ( cnt == effac@tcg ) { continue }
				}
				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_SAMEDBID ) {
					if ( card@tcg(TCG_CARD_REF_DBID, cnt) != card@tcg(TCG_CARD_REF_DBID, effac@tcg) ) { continue }
