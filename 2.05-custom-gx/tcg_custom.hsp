				if ( effref@tcg(TCG_SKILL_FILTER, eff@tcg) == TCG_SKILL_FILTER_SAMEDBIDNOTSELF ) {
					if ( card@tcg(TCG_CARD_REF_DBID, cnt) != card@tcg(TCG_CARD_REF_DBID, effac@tcg) | cnt == effac@tcg ) { continue }
