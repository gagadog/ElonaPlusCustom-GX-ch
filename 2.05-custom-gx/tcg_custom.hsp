	targetdeck@tcg = 0
	rndrace@tcg = ""
	rndclass@tcg = ""
	rndsex@tcg = ""
	rnddomain@tcg = 0
	rndlvl@tcg = 0
	rndfixlvl@tcg = 0 // 2 OR 6
	return

