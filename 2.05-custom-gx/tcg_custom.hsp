*card_loadplayerdeck
	curdeck = limit(gdata(GDATA_DECK), 0, 4)
	cardsize@tcg = 0
	file = "" + exedir + "tmp\\deck_" + curdeck + ".s2"
	existwrapper file
	if ( rtval != (-1) ) { 
		fmode = 24
		gosub *game_ctrlFile@
		repeat MAX_CREATURE_ID - 3, 1
			cnt2@tcg = cnt
			if ( card(0, cnt2@tcg) < deck(cnt2@tcg) ) {
				deck(cnt2@tcg) = card(0, cnt2@tcg)
			}
			if ( deck(cnt2@tcg) <= 0 ) {
				continue
			}
			repeat deck(cnt2@tcg)
				if ( cardsize@tcg > MAX_CARD_DECK ) {
					break
				}
				card_ref cnt2@tcg
				if ( stat >= 0 ) {
					create_card cardsize@tcg, stat
					card@tcg(TCG_CARD_LOCATION, cardsize@tcg) = TCG_LOCATION_DECK, rp@tcg
					cardsize@tcg++
					if ( dbg_tcg ) {
						card@tcg(TCG_CARD_COST, cardsize@tcg) = 0
						card@tcg(TCG_CARD_REF_COST, cardsize@tcg) = 0
					}
				}
			loop
		loop
	}
	return

*card_generateopponentdeck
	cardsize@tcg = 0
	dim deck_opponent, MAX_CARD
	if ( cdata(CDATA_QUALITY, tc) == FIX_QUALITY_UNIQUE | (cdata(CDATA_ID, tc) == CREATURE_ID_USER & cdata(CDATA_QUALITY, tc) >= FIX_QUALITY_MIRACLE) ) {
		gosub *card_populate_deck_unique
		if ( stat == 1 ) { 
			repeat MAX_CREATURE_ID - 3, 1
				cnt2@tcg = cnt
				if ( cnt == 0 ) { continue }
				if ( deck_opponent(cnt) <= 0 ) { continue }
				repeat deck_opponent(cnt)
					cc@tcg = MAX_CARD_DECK_TOTAL + cardsize@tcg
					create_card cc@tcg, cnt2@tcg
					card@tcg(TCG_CARD_LOCATION, cc@tcg) = TCG_LOCATION_DECK, rp@tcg
					cardsize@tcg++
				loop
				if ( cardsize@tcg >= MAX_CARD_DECK ) { break }
			loop
			return 
		}
	}
	card_ref cdata(CDATA_ID, tc)
	deckuserdomain@tcg = cardrefdomain + 1
	if ( deckuserdomain@tcg == 5 | deckuserdomain@tcg == 6 ) {
		deckuserdomain@tcg = rnd(6) + 1
		if ( deckuserdomain@tcg >= 5 ) {
			deckuserdomain@tcg += 2
		}
	}
	dueldecksize@tcg = MIN_CARD_DECK + rnd(rnd(20)+1)
	duelrace@tcg = cdatan(CDATAN_RACE, tc) 
	randomize tc * 10000 + cdata(CDATA_AGE, tc) 
	// Town NPCs have random decks
	if ( cdata(CDATA_ROLE, tc) > ROLE_NONE & cdata(CDATA_RELATION, tc) == RELATION_NEUTRAL ) {
		duelrace@tcg == "god"
	}
	// These decks has too little racial cards.
	if ( duelrace@tcg == "god" | duelrace@tcg == "doggod" | duelrace@tcg == "catgod" | duelrace@tcg == "machinegod" | duelrace@tcg == "undeadgod" | duelrace@tcg == "servant" | duelrace@tcg == "quickling" | duelrace@tcg == "dwarf" | duelrace@tcg == "catsister" | duelrace@tcg == "metal" ) {
		if ( rnd(1) == 0 ) { duelrace@tcg = "yerles" }
		if ( rnd(2) == 0 ) { duelrace@tcg = "eulderna" }
		if ( rnd(3) == 0 ) { duelrace@tcg = "elea" }
		if ( rnd(4) == 0 ) { duelrace@tcg = "juere" }
		if ( rnd(5) == 0 ) { duelrace@tcg = "zanan" }
		if ( rnd(6) == 0 ) { duelrace@tcg = "karune" }
		if ( rnd(10) == 0 ) { duelrace@tcg = "roran" }
		deckuserdomain@tcg = 3
	}
	// Generic humanoid have a chance to not use their race's deck.
	if ( rnd(5) == 0 ) {
		if ( duelrace@tcg == "yerles" | duelrace@tcg == "eulderna" | duelrace@tcg == "elea" | duelrace@tcg == "juere" | duelrace@tcg == "zanan" ) {
			duelrace@tcg = "norland"
		}
	}
	// norland/karune have a sucky system due to it's card pool, they'd use a somewhat more consistent deck.
	if ( duelrace@tcg == "karune" | duelrace@tcg == "norland" ) {
		if ( rnd(1) == 0 ) {
			if ( rnd(1) == 0 ) { duelrace@tcg = "seamonster" }
			if ( rnd(2) == 0 ) { duelrace@tcg = "spirit" }
			if ( rnd(3) == 0 ) { duelrace@tcg = "mushroom" }
			if ( rnd(4) == 0 ) { duelrace@tcg = "wisp" }
			if ( rnd(5) == 0 ) { duelrace@tcg = "wasp" }
			deckuserdomain@tcg = 1
		}
		if ( rnd(2) == 0 ) {
			if ( rnd(1) == 0 ) { duelrace@tcg = "bear" }
			if ( rnd(2) == 0 ) { duelrace@tcg = "ent" }
			if ( rnd(3) == 0 ) { duelrace@tcg = "dog" }
			if ( rnd(4) == 0 ) { duelrace@tcg = "fairy" }
			if ( rnd(5) == 0 ) { duelrace@tcg = "bird" }
			deckuserdomain@tcg = 2
		}
		if ( rnd(3) == 0 ) {
			if ( rnd(1) == 0 ) { duelrace@tcg = "minotaur" }
			if ( rnd(2) == 0 ) { duelrace@tcg = "lizardman" }
			if ( rnd(3) == 0 ) { duelrace@tcg = "orc" }
			if ( rnd(4) == 0 ) { duelrace@tcg = "kobolt" }
			if ( rnd(5) == 0 ) { duelrace@tcg = "yeek" }
			if ( rnd(6) == 0 ) { duelrace@tcg = "rat" }
			deckuserdomain@tcg = 7
		}
		if ( rnd(4) == 0 ) {
			if ( rnd(1) == 0 ) { duelrace@tcg = "imp" }
			if ( rnd(2) == 0 ) { duelrace@tcg = "yith" }
			if ( rnd(3) == 0 ) { duelrace@tcg = "drake" }
			if ( rnd(4) == 0 ) { duelrace@tcg = "eye" }
			if ( rnd(5) == 0 ) { duelrace@tcg = "asura" }
			deckuserdomain@tcg = 8
		}
		if ( rnd(5) == 0 ) {
			if ( rnd(1) == 0 ) { duelrace@tcg = "zombie" }
			if ( rnd(2) == 0 ) { duelrace@tcg = "ghost" }
			if ( rnd(3) == 0 ) { duelrace@tcg = "piece" }
			if ( rnd(4) == 0 ) { duelrace@tcg = "lich" }
			if ( rnd(5) == 0 ) { duelrace@tcg = "skeleton" }
			if ( rnd(6) == 0 ) { duelrace@tcg = "worm" }
			deckuserdomain@tcg = 4
		}
		if ( rnd(6) == 0 ) {
			if ( rnd(1) == 0 ) { duelrace@tcg = "yerles" }
			if ( rnd(2) == 0 ) { duelrace@tcg = "eulderna" }
			if ( rnd(3) == 0 ) { duelrace@tcg = "elea" }
			if ( rnd(4) == 0 ) { duelrace@tcg = "juere" }
			if ( rnd(5) == 0 ) { duelrace@tcg = "cupid" }
			if ( rnd(10) == 0 ) { duelrace@tcg = "roran" }
			deckuserdomain@tcg = 3
		}
	}
	randomize
	repeat dueldecksize@tcg
		gosub *card_filter_reset
		rndlvl@tcg = cdata(CDATA_LEVEL, tc) + 80
		targetdeck@tcg = 2 
		if ( rnd(3) > 0 ) { 
			rndfixlvl@tcg = 2
			rndrace@tcg = duelrace@tcg
			if ( rndrace@tcg == "yerles" & rnd(4) == 0 ) { rndrace@tcg = "machine" }
			if ( rndrace@tcg == "dog" & rnd(40) == 0 ) { rndrace@tcg = "doggod" }
			if ( rndrace@tcg == "cat" & rnd(40) == 0 ) { rndrace@tcg = "catgod" }
			if ( rndrace@tcg == "lich" & rnd(40) == 0 ) { rndrace@tcg = "undeadgod" }
			if ( rndrace@tcg == "machine" & rnd(40) == 0 ) { rndrace@tcg = "machinegod" }
			if ( rndrace@tcg == "bird" & rnd(4) == 0 ) { rndrace@tcg = "harpy" }
			if ( rndrace@tcg == "harpy" & rnd(4) == 0 ) { rndrace@tcg = "bird" }
			if ( rndrace@tcg == "cupid" & rnd(4) == 0 ) { rndrace@tcg = "quickling" }
		} else {
			if ( rnd(5) > 0 ) {
				rndfixlvl@tcg = 2
				rnddomain@tcg = deckuserdomain@tcg
			} else {
				if ( rnd(5) != 0 ) {
					rnddomain@tcg = 5
				} else {
					rnddomain@tcg = 6
				}
			}
		}
		gosub *card_filter
		if ( cardno <= 0 | cardno > MAX_CREATURE_ID - 3 ) { continue }
		deck_opponent(cardno)++
		if ( cardno == CREATURE_ID_YERLES_MACHINE_INFANTRY ) {
			if (deck_opponent(CREATURE_ID_YERLES_COMBAT_PLANE) < 4 ) { deck_opponent(CREATURE_ID_YERLES_COMBAT_PLANE)++ }
		}
		if ( cardno == CREATURE_ID_ELDER ) {
			if (deck_opponent(CREATURE_ID_GUARD) < 2 ) { deck_opponent(CREATURE_ID_GUARD) = 2 }
			if (deck_opponent(CREATURE_ID_GUARD2) < 2 ) { deck_opponent(CREATURE_ID_GUARD2) = 2 }
		}
		if ( cardno == CREATURE_ID_CAPTAIN ) {
			if (deck_opponent(CREATURE_ID_SAILOR) < 2 ) { deck_opponent(CREATURE_ID_SAILOR) = 2 }
		}
	loop
	create_card MAX_CARD_DECK_TOTAL, cdata(CDATA_ID, tc)
	cc@tcg = MAX_CARD_DECK_TOTAL + 1
	repeat MAX_CREATURE_ID - 3, 1
		cnt2@tcg = cnt
		if ( deck_opponent(cnt2@tcg) > 0 ) {
			repeat deck_opponent(cnt2@tcg)
				create_card cc@tcg, cnt2@tcg
				card@tcg(TCG_CARD_LOCATION, cc@tcg) = TCG_LOCATION_DECK, rp@tcg
				cc@tcg++
				cardsize@tcg++
			loop
		}
	loop
	return

#deffunc tcgreward
	gdata(GDATA_MIN) += 5
	if ( gdata(GDATA_TIME_LIMIT) > 0 & gdata(GDATA_AREA) == AREA_QUEST ) {
		if ( gdata(GDATA_QUEST) != QUEST_TYPE_MINEFIELD ) {
			gdata(GDATA_TIME_LIMIT) -= 5
			if ( gdata(GDATA_TIME_LIMIT) <= 0 ) {
				gdata(GDATA_TIME_LIMIT) = 0
				qexit = 100
				evadd EVENT_QUEST_TIMEOUT
			}
		}
	}
	if ( gameresult@tcg > 0) {
		if ( gameresult@tcg \ 2 == 1 ) {
			cdata(CDATA_EMO_ICON, cc) = 6 + 100 * 3
			cdata(CDATA_EMO_ICON, tc) = 13 + 100 * 3
			modimp tc, 7
			cdata(CDATA_INTEREST, tc) -= 80
			cdata(CDATA_INTEREST_RENEW, tc) = gdata(GDATA_HOUR) + gdata(GDATA_DAY) * 24 + gdata(GDATA_MONTH) * 24 * 30 + gdata(GDATA_YEAR) * 24 * 30 * 12 + 12
			winconsecutive@tcg++
			txtef COLOR_YELLOW
			txt "" + winconsecutive@tcg + " consecutive wins!"
			ticketreward@tcg = limit(limit(winconsecutive@tcg, 1, 10) * 20, 1, cdata(CDATA_LEVEL, tc))
			txtef COLOR_YELLOW
			txt "You get " + ticketreward@tcg + " music tickets!"
			flt
			itemcreate -1, ITEM_ID_MUSIC_TICKET, cdata(CDATA_X, CHARA_PLAYER), cdata(CDATA_Y, CHARA_PLAYER), ticketreward@tcg
			if ( cdata(CDATA_QUALITY, tc) == FIX_QUALITY_UNIQUE | (cdata(CDATA_ID, tc) == CREATURE_ID_USER & cdata(CDATA_QUALITY, tc) >= FIX_QUALITY_MIRACLE) ) {
				winagainstunique@tcg++
				txtef COLOR_YELLOW
				txt "" + winagainstunique@tcg + " wins against Unique NPCs!"
				if ( winagainstunique@tcg \ 5 == 0 ) {
					txtef COLOR_YELLOW
					txt "You get a Card Pack!"
					flt
					itemcreate -1, ITEM_ID_CARD_PACK, cdata(CDATA_X, CHARA_PLAYER), cdata(CDATA_Y, CHARA_PLAYER), 1
				}
			}
			cell_refresh cdata(CDATA_X, CHARA_PLAYER), cdata(CDATA_Y, CHARA_PLAYER)
		} else {
